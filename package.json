{
  "name": "node-proxy-test",
  "version": "0.0.1",
  "description": "Some useful things, maybe :)",
  "main": "./build/main.js",
  "author": "remipassmoilesel",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/remipassmoilesel/node-proxy-test.git",
  "scripts": {
    "cli": "npm run clean-compile && node build/main.js",
    "cli-debug": "npm run clean-compile && DEBUG=express:* node build/main.js",
    "clean-generated": "rm src/generated-tests/*.ts",
    "clean": "node_modules/.bin/rimraf build",
    "compile": "node_modules/.bin/tsc --pretty",
    "compile-watch": "node_modules/.bin/tsc --pretty -w",
    "clean-compile": "npm run clean && npm run compile",
    "test": "npm run clean-compile && node_modules/.bin/mocha --recursive 'build/tests/**/*Test.js'",
    "generated-tests": "npm run clean-compile && node_modules/.bin/mocha --recursive 'build/generated-tests/**/*Spec.js'",
    "coverage": "npm run clean-compile && node_modules/.bin/nyc node_modules/.bin/mocha --full-trace --recursive 'build/manager/api/tests/**/*Test.js'",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "tslint -p .",
    "lint-fix": "tslint -p . --fix"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "docker-names": "^1.0.3",
    "express": "^4.16.2",
    "f-promise": "^1.2.11",
    "got": "^8.2.0",
    "http-proxy": "^1.16.2",
    "mustache": "^2.3.0",
    "node-uuid": "^1.4.8",
    "rimraf": "^2.6.2",
    "socksv5": "0.0.6",
    "source-map-support": "^0.5.3",
    "to-camel-case": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/colors": "^1.1.3",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^4.0.3",
    "@types/got": "^7.1.7",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.43",
    "@types/mustache": "^0.8.30",
    "@types/node": "^8.9.4",
    "@types/node-uuid": "0.0.28",
    "@types/sinon": "^2.3.6",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "f-mocha": "^1.1.3",
    "lodash": "^4.17.5",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.5.3"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "cache": false
  }
}
